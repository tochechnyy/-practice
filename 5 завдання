#include <stdlib.h>
#include <stdio.h>

#define MOD 12345 // Модуль

int main(){
    int n, i, dp[n + 1];
    dp[0] = 1;// Початкові значення
    dp[1] = 2;// Початкові значення
    dp[2] = 4;// Початкові значення

    printf("Введіть значенння nвід 1 до 10000: ");
    scanf("%d", &n);
    if (n < 1 || n > 10000) {
        // Перевірка коректності
        printf("\nПОМИЛКА!!!!!");
        exit(0);
    }
    for (i = 3; i <= n; i++) {
        // Обчислення dp[i]
        dp[i] = (dp[i-1] + dp[i-2] + dp[i-3]) % MOD;
    }
    printf("Кількість шуканих послідовностей за модулем 12345: %d\n", dp[n]);

}
